								};
								if (current_site['is_bought_theme_bitch']) {
									that.trade.theme_bitch = current_site['trade_bitch'];
									var current_trade_theme_bitch_branch = that.branch_all[that.trade.theme_bitch['branch_id']];
									if (current_trade_theme_bitch_branch) {
										that.bought_type.theme_bitch = 1;
										that.trade.theme_bitch_branch = current_trade_theme_bitch_branch;
										that.trade.theme_bitch_sort = that.trade.theme_bitch_branch['sort'];
										bought_sorts.push(that.trade.theme_bitch_sort);
