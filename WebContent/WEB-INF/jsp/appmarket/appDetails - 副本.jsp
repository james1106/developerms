							} else {
								if (current_site['is_bought_theme']) {
									that.trade.theme = current_site['trade_theme'];
									var current_trade_theme_branch = that.branch_all[that.trade.theme['branch_id']];
									if (current_trade_theme_branch) {
										that.bought_type.theme = 1;
										that.trade.theme_branch = current_trade_theme_branch;
										that.trade.theme_branch_sort = that.trade.theme_branch['sort'];
										bought_sorts.push(that.trade.theme_branch_sort);
