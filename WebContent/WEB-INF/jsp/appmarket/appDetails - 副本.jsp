							that.current_branch_support = {
								'app' : that.current_branch['app_support'] == 2 ? 1 : 0,
								'wxapp' : that.current_branch['wxapp_support'] == 2 ? 1 : 0,
							};
							var app_support = that.current_branch_support.app;
							var wxapp_support = that.current_branch_support.wxapp;
							that.select_type = {
								app : app_support,
								wxapp : wxapp_support,
								app_selected : app_support,
								wxapp_selected : wxapp_support,
							};
						};
					},
					branchCheck : function(branch) {
						if (branch['status'] == 1 && branch['show'] == 1 && branch['force_upgrade'] == 0 && branch['private'] == this.private) {
							return 1;
						} else {
							return 0;
						};
					},
					branchListFilter : function(bought_branch_sort_max) {
						var that = this;
						var default_branch_id = 0;
						if (!bought_branch_sort_max) {
							if (!that.current_branch_id) {
								that.current_branch_id = current_branch_id;
							};
							that.branches_list = that.branch_list;
						} else {
							var branch_list_new = [];
							
							for (var i in that.branch_all_list) {
								var branch = that.branch_all_list[i];
								if (that.branchCheck(branch) && branch['sort'] >= bought_branch_sort_max) {
									branch_list_new.push(branch);
								};
								if (branch['sort'] == bought_branch_sort_max) {
									default_branch_id = branch['id'];
								};
							};
							that.branches_list = branch_list_new;
							if (default_branch_id && default_branch_id != that.current_branch_id) {
								var new_branch = that.branch_all[default_branch_id];
								if (!that.branchCheck(new_branch)) {
									var new_default_branch_id = 0;
									for (var i in that.branch_all_list) {
										var branch = that.branch_all_list[i];
										if (!new_default_branch_id && that.branchCheck(branch) && branch['sort'] > bought_branch_sort_max) {
											new_default_branch_id = branch['id'];
										};
									};
									that.current_branch_id = new_default_branch_id;
								} else {
									that.current_branch_id = default_branch_id;
								};
							};
						};
						// 分支过滤后重新获取
						that.branchInit();
